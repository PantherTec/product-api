{
  "name": "practice",
  "version": "1.0.0",
  "description": "simple api for issuing traffic tickets",
  "main": "app.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "lint": "eslint src/ --fix",
    "test": "jest",
    "start": "NODE_ENV=development npm run clusters",
    "clusters": "babel-node cluster.js",
    "create-db": "sequelize db:create",
    "migrate": "sequelize db:migrate",
    "setup-db": "npm run create-db && npm run migrate",
    "create-migration": "sequelize migration:create --name ",
    "create-seed": "sequelize seed:generate --name ",
    "reset-db": "sequelize db:migrate:undo:all",
    "undo-migration": "sequelize db:migrate:undo",
    "seed-db": "sequelize db:seed:all",
    "precommit": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:tilte/core-svc.git"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "express-validator": "^5.0.0",
    "helmet": "^3.13.0",
    "http-status": "^1.0.1",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.3",
    "node-fetch": "^1.7.3",
    "sequelize": "^4.8.0",
    "sequelize-cli": "^4.0.0",
    "spdy": "^3.4.7",
    "through2": "^2.0.3",
    "uuid": "^3.2.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-jest": "^23.4.2",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "regenerator-runtime": "^0.12.1",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": "10.0.0"
  }

}
